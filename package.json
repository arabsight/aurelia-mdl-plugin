{
    "name": "aurelia-mdl-plugin",
    "version": "1.0.0",
    "description": "Material Design Lite plugin for Aurelia.",
    "keywords": [
        "aurelia",
        "plugin",
        "mdl",
        "material design"
    ],
    "scripts": {
        "prepush": "gulp prepare-release --bump=major",
        "push": "git add --all && git commit -m \"$(date)\" && git push origin master && npm publish",
        "test": "gulp test"
    },
    "homepage": "https://github.com/arabsight/aurelia-mdl-plugin",
    "bugs": {
        "url": "https://github.com/arabsight/aurelia-mdl-plugin/issues"
    },
    "license": "MIT",
    "author": "Rabah Ghodbane <r-ghodbane@live.com>",
    "main": "dist/commonjs/index.js",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/arabsight/aurelia-mdl-plugin"
    },
    "devDependencies": {
        "aurelia-pal": "^1.3.0",
        "aurelia-tools": "^2.0.0",
        "babel": "^6.5.2",
        "babel-eslint": "^10.1.0",
        "babel-plugin-syntax-flow": "^6.13.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-es2015-modules-amd": "^6.8.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.14.0",
        "babel-plugin-transform-es2015-modules-systemjs": "^6.14.0",
        "babel-plugin-transform-es2015-spread": "^6.8.0",
        "babel-plugin-transform-flow-strip-types": "^6.14.0",
        "babel-preset-es2015": "^6.14.0",
        "babel-preset-es2015-loose": "^8.0.0",
        "babel-preset-stage-1": "^6.13.0",
        "conventional-changelog": "^3.1.23",
        "del": "^6.0.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^6.1.2",
        "gulp-bump": "^3.2.0",
        "gulp-eslint": "^3.0.1",
        "gulp-yuidoc": "^0.1.2",
        "isparta": "^4.0.0",
        "istanbul": "^1.1.0-alpha.1",
        "jasmine-core": "^3.6.0",
        "karma": "^6.3.16",
        "karma-babel-preprocessor": "^8.0.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.3",
        "karma-jasmine": "^4.0.1",
        "karma-jspm": "^2.2.0",
        "object.assign": "^4.0.4",
        "require-dir": "^1.2.0",
        "run-sequence": "^2.2.1",
        "vinyl-paths": "^3.0.1",
        "yargs": "^16.0.3"
    },
    "jspm": {
        "registry": "npm",
        "main": "index",
        "format": "amd",
        "directories": {
            "dist": "dist/amd"
        },
        "devDependencies": {
            "aurelia-framework": "^1.0.0",
            "aurelia-polyfills": "^1.0.0"
        }
    },
    "aurelia": {
        "build": {
            "resources": [
                "mdl"
            ]
        }
    }
}
